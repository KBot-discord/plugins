{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "docs": {
      "dependsOn": ["build"],
      "outputs": ["docs/**"]
    },
    "lint": {}
  }
}

